{
  "name": "veiaco-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "graphql": "^14.5.8",
    "i18next": "^17.0.13",
    "i18next-browser-languagedetector": "^3.0.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-i18next": "^10.12.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-select": "^3.0.8",
    "react-toastify": "^5.3.2",
    "styled-components": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint:css": "stylelint ./src/**/*styles.js",
    "coverage": "react-scripts test --coverage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.4.0",
    "handlebars": "^4.5.0",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/styles.js",
      "!src/index.js",
      "!src/i18n.js",
      "!src/serviceWorker.js",
      "!**/locales/**",
      "!**/graphql/**",
      "!src/setupTests.js",
      "!src/routes/index.js",
      "!node_modules/"
    ]
  }
}
